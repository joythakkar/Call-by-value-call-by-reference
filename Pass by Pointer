#include <iostream>
using namespace std;
 void swap(int *x, int *y){
        int temp=*x;
        *x=*y;
        *y=temp;
        return ;
    }

int main() {
    int a=5; int b=2;
    
    cout<<"Original value of a is:"<<a<<endl;
    
    cout<<"original value of b is:"<<b<<endl;
    swap(&a,&b);
    cout<<"Swapped value of a is:"<<a<<endl;
    
    cout<<"Swapped value of b is:"<<b<<endl;

    return 0;
}
